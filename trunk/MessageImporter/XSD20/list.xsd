				<xsd:documentation>Párovat dle párovacího symbolu.</xsd:documentation>
			</xsd:annotation>
		</xsd:enumeration>
		<xsd:enumeration value="PairingSymbolIC">
			<xsd:annotation>
				<xsd:documentation>Párovat dle párovacího symbolu a IÈ.</xsd:documentation>
			</xsd:annotation>
		</xsd:enumeration>		
	</xsd:restriction>
