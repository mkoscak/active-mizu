				<xsd:documentation>Typ práce s dokladem. Výchozí hodnota je pøidání nového dokladu.</xsd:documentation>
			</xsd:annotation>
		</xsd:element>
		<xsd:element name="intParameterID" type="typ:idType" minOccurs="0" maxOccurs="1">
		  <xsd:annotation>
				<xsd:documentation>ID internetového parametru.</xsd:documentation>
			</xsd:annotation>
		</xsd:element>	
		<xsd:element name="intParameterName" type="typ:string255" minOccurs="0" maxOccurs="1">
		  <xsd:annotation>
				<xsd:documentation>Název internetového parametru.</xsd:documentation>
			</xsd:annotation>
		</xsd:element>		
